		<httpRuntime requestValidationMode="2.0"/>
	</system.web>
	<system.web.extensions/>
  
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true">
		</modules>
		<handlers>
			<remove name="UrlRoutingHandler"/>
